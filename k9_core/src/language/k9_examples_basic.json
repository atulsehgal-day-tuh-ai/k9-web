      },

      "system_result": {
        "analysis": {
          "trend": "increasing",
          "risk_id": "R01"
        }
      },

      "expected_synthesis": "En las últimas semanas, el riesgo R01 ha mostrado una tendencia de aumento.",

      "rules_applied": [
        "Lenguaje direccional",
        "No se explican causas",
        "No se cuantifica sin datos explícitos"
      ]
    }
