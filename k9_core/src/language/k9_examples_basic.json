    {
      "example_id": "BASIC_11",
      "example_type": "simple",
      "question_type": "comparative_analysis",

      "human_input": "¿Qué riesgos del modelo K9 está subestimando el modelo proactivo?",

      "interpretation_output": {
        "type": "K9_COMMAND",
        "intent": "COMPARATIVE_QUERY",
        "entity": "risks",
        "operation": "compare",
        "filters": {
          "scope": "K9_CORE",
          "model": "proactive_model",
          "baseline": "K9"
        },
        "output": "analysis"
      },

      "system_result": {
        "analysis": {
          "underestimated_risks": ["R02"]
        }
      },

      "expected_synthesis": "El modelo proactivo está subestimando el riesgo R02 en comparación con la evaluación realizada por K9.",

      "rules_applied": [
        "Comparación directa",
        "Énfasis en diferencias",
        "No se agregan juicios de valor"
      ]
    },

    {
      "example_id": "BASIC_12",
