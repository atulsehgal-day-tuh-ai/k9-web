{
  "examples": [

    {
      "example_id": "BASIC_01",
      "example_type": "simple",
      "question_type": "ontology_descriptive",

      "human_input": "¿Qué es el riesgo R01?",

      "interpretation_output": {
        "type": "K9_COMMAND",
        "intent": "ONTOLOGY_QUERY",
        "entity": "risk",
        "operation": "retrieve",
        "filters": { "risk_id": "R01" },
        "output": "summary"
      },

      "system_result": {
        "analysis": {
          "risk_id": "R01",
          "name": "Caída de altura",
          "description": "Exposición a caída desde un nivel superior con potencial de lesiones graves o fatales."
        }
      },

      "expected_synthesis": "El riesgo R01 corresponde a la caída de altura, asociada a la exposición a desniveles con potencial de generar lesiones graves o fatales.",

      "rules_applied": [
        "Pregunta descriptiva",
        "Respuesta directa",
        "No se agrega interpretación",
        "Se utiliza solo la información entregada por el sistema"
      ]
    },

    {
      "example_id": "BASIC_02",
      "example_type": "simple",
      "question_type": "ontology_descriptive",

      "human_input": "Descríbeme el riesgo de caída de altura",

      "interpretation_output": {
