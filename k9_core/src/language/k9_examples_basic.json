      "rules_applied": [
        "Comparación relativa",
        "No se detallan puntajes",
        "Se prioriza diferencia relevante"
      ]
    },

    {
      "example_id": "BASIC_13",
      "example_type": "simple",
      "question_type": "temporal_relation",

      "human_input": "¿Qué pasó antes y después del lunes crítico?",

      "interpretation_output": {
        "type": "K9_COMMAND",
        "intent": "TEMPORAL_RELATION_QUERY",
        "entity": "signals",
        "operation": "sequence",
        "filters": { "anchor_event": "CRITICAL_MONDAY" },
        "time": { "type": "window", "value": "pre_post" },
        "output": "analysis"
      },

      "system_result": {
        "analysis": {
          "before": "incremento progresivo de señales de exposición",
          "after": "evento crítico seguido de múltiples controles fallidos"
        }
      },

      "expected_synthesis": "Previo al lunes crítico se observó un incremento progresivo de señales de exposición. Posteriormente, se registró un evento crítico acompañado por múltiples fallas en controles.",

      "rules_applied": [
        "Relación temporal",
        "Orden secuencial",
        "No se infiere causalidad adicional"
      ]
    },

