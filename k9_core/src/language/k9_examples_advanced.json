    {
      "example_id": "ADV_01",
      "example_type": "advanced",
      "question_type": "analytical_ranking_with_explanation",

      "human_input": "¿Cuál es el riesgo más importante esta semana y por qué?",

      "interpretation_output": {
        "type": "K9_COMMAND",
        "intent": "ANALYTICAL_QUERY",
        "entity": "risks",
        "operation": "rank",
        "filters": { "scope": "K9_CORE" },
        "time": { "type": "relative", "value": "current_week" },
        "output": "analysis",
        "include_explanation": true
      },

      "system_result": {
        "analysis": {
          "ranked_risks": [
            {
              "risk_id": "R02",
              "score": 0.88,
              "drivers": [
                "exposure_accumulation",
                "control_degradation"
              ]
            },
            {
              "risk_id": "R01",
              "score": 0.62
            }
          ]
        }
      },

      "expected_synthesis": "Durante esta semana, el riesgo con mayor nivel de criticidad fue R02. Este comportamiento estuvo asociado principalmente a la acumulación de exposiciones operacionales y a la degradación de controles críticos.",

      "rules_applied": [
        "Primero se informa el resultado",
        "Luego se explica usando solo drivers entregados",
        "No se comparan puntajes explícitos"
      ]
    },

